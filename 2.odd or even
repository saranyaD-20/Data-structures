#include<stdio.h>
main()
 {
	int numcount;
	printf("enter a number of elements:");
	scanf("%d",&numcount);
	printf("enter %d numbers:\n",numcount);
	for(int i=0;i<numcount;i++)
	{
	    int n;
	    scanf("%d",&n);
		if(n%2==0)
		{
			printf("even number",n);
		}
		else{
			printf("odd number",n);
		}
	}
}
